{
  "name": "WhitePages PRO Reverse Phone Lookup.",
  "version": "0.1.1",
  "description": "WhitePages PRO Reverse Phone Lookup.",
  "icons": { "128": "images/rsz_wp_128.png" },
  "background": { "scripts": ["background.js"] },
    "web_accessible_resources": [
